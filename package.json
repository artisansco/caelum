{
  "name": "caelum",
  "version": "0.0.1",
  "description": "A web application for schools",
  "private": true,
  "type": "module",
  "engineStrict": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "postbuild": "cp -r package.json *.sql drizzle build/",
    "start": "node build/",
    "preview": "vite preview",
    "format": "prettier --write ./**/*.svelte && biome check --write .",
    "prepare": "svelte-kit sync || echo ''",
    "test": "echo \"no node:test test implemented yet\"",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db:seed": "npx tsx ./src/lib/server/database/seed.ts",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:drop": "drizzle-kit drop",
    "db:studio": "drizzle-kit studio"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@iconify-json/lucide": "^1.2.71",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify/svelte": "^5.0.2",
    "@iconify/tailwind4": "^1.0.6",
    "@melt-ui/pp": "^0.3.2",
    "@melt-ui/svelte": "^0.86.6",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.48.3",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.16",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.2",
    "@types/node-cron": "^3.0.11",
    "drizzle-kit": "^0.31.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.43.0",
    "svelte-check": "^4.3.3",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  },
  "dependencies": {
    "@formkit/tempo": "^0.1.2",
    "@tiptap/extension-link": "^3.9.1",
    "@tiptap/starter-kit": "^3.9.1",
    "africastalking": "^0.7.7",
    "basecoat-css": "^0.3.2",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.4.1",
    "chart.js": "^4.5.1",
    "cloudinary": "^2.8.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "drizzle-seed": "^0.3.1",
    "isomorphic-dompurify": "^2.30.1",
    "jsonwebtoken": "^9.0.2",
    "melt": "^0.41.0",
    "nanoid": "^5.1.6",
    "node-cron": "^4.2.1",
    "papaparse": "^5.5.3",
    "pdfkit": "^0.17.2",
    "resend": "^6.3.0",
    "sharp": "^0.34.4",
    "svelte-sonner": "^1.0.5",
    "valibot": "^1.1.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artisansco/caelum.git"
  },
